  "description": "A styleguide for the Lean Theme's UI components",
  "homepage": "https://github.com/wearenolte/lean-theme-styleguide",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Nolte",
      "email": "developer@wearenolte.com",
      "homepage": "https://wearenolte.com/"
    },
